   "source": [
    "# Python program to print squares of all Even Numbers in the range of 100 to 200\n",
    "\n",
    "start = 100\n",
    "last = 200\n",
    "\n",
    "# iterating each number in list\n",
    "for num in range(start, last + 1):\n",
    "\n",
    "\t# checking condition\n",
    "\tif num % 2 == 0:       \n",
    "\t\tprint(num**2,end=\" \")\n"
   ]
  }
